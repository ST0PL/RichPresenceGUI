                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="themeSwitch_background"
                        Color="#1b1b1d"/>
    <SolidColorBrush x:Key="themeSwitch_circle"
                        Color="#FFFFFF"/>
    <SolidColorBrush x:Key="light_themeSwitch_background"
                        Color="#bebebe"/>
