<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="clr-namespace:ControlsLib.Controls">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/ControlsLib;component/Styles/ButtonStyle.xaml"/>
        <ResourceDictionary Source="/ControlsLib;component/Styles/CheckBoxStyle.xaml"/>
        <ResourceDictionary Source="/ControlsLib;component/Styles/ComboBoxStyle.xaml"/>
        <ResourceDictionary Source="/ControlsLib;component/Styles/ImageRadioButtonStyle.xaml"/>
        <ResourceDictionary Source="/ControlsLib;component/Styles/NavigationButtonStyle.xaml"/>
        <ResourceDictionary Source="/ControlsLib;component/Styles/ListViewStyle.xaml"/>
        <ResourceDictionary Source="/ControlsLib;component/Styles/ProgressBarStyle.xaml"/>
